    "@nestjs/testing": "^11.0.0",
    "@nx/angular": "21.6.5",
    "@nx/devkit": "21.6.5",
    "@nx/eslint": "21.6.5",
    "@nx/eslint-plugin": "21.6.5",
    "@nx/jest": "21.6.5",
